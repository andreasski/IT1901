<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<BorderPane fx:id="body" fx:controller="frontend.GUI.BoB_Controller" xmlns:fx="http://javafx.com/fxml" stylesheets="@../css/bob.css">

    <top>
        <fx:include source="bob_header.fxml"/>
    </top>

    <left>

        <VBox>
            <HBox fx:id="nav">
                <Label fx:id="lvl1" styleClass="underline"/>
                <Label fx:id="Spacing_lvl1_lvl2"/>
                <Label fx:id="lvl2" styleClass="underline"/>
            </HBox>

            <VBox fx:id="aside">
                <VBox styleClass="margin">
                    <Label fx:id="instructionBoxLabel" styleClass="textContainer"/>
                </VBox>
                <VBox styleClass="margin">
                    <Button fx:id="setPrice" text="Sett pris"/>
                </VBox>
            </VBox>
        </VBox>
    </left>

    <center>
        <VBox fx:id="container" styleClass="margin">
        </VBox>
    </center>

</BorderPane>