<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane fx:id="body" fx:controller="frontend.GUI.Man_Controller" xmlns:fx="http://javafx.com/fxml" stylesheets="@../css/man.css">

    <top>
        <fx:include source="man_header.fxml"/>
    </top>

    <left>
        <VBox>
            <VBox fx:id="instructionBoxContainer" styleClass="margin">
                <StackPane styleClass="textContainer">
                    <Label fx:id="instructionBoxLbl"/>
                </StackPane>
            </VBox>
        </VBox>
    </left>

    <center>
        <VBox fx:id="container" styleClass="margin">
        </VBox>
    </center>

</BorderPane>